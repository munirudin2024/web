sudo -i
sudo service postgresql start
sudo -u postgres psql

begin; // memulai transaksi
masukan perintah;
masukan perintah;
masukan perintah salah;
roollback; // mengembalikan database ke keadaan sebelum transaksi dimulai.
masukan perintah;
commit; // simpan secara permanen ke database
